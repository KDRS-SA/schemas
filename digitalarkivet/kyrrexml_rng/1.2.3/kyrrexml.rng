        <ref name="attlist.global"/>
    <empty/>
  </define>
  
  <!-- 2013-10-07 TMB, Digitalarkivet: Legge til ID fra RHD, som skal taes vare på i filer fra dem. -->
  <define name="RHD_ID">
    <element name="RHD_ID">
