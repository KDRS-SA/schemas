<?xml version="1.0" encoding="UTF-8"?>
<!--
  DTD for KyrreML - hovudformatet for data som foelgjer Kyrre - Standard for dataregistrering av kyrkjeboeker i Noreg.
  
  Versjon 1.1, 17.06.2011 (utvida versjon utarbeidd av Lars Nygaard, Riksarkivet)
  
  Relax NG skjema for KyrreML - hovudformatet for data som foelgjer Kyrre - Standard for dataregistrering av kyrkjeboeker i Noreg.
  Versjon 1.2, 05.10.2011 utvida versjon utarbeidd av Tone Merete Bruvik, Digitalarkivet, Statsarkivet i Bergen.
      -  rettelse 27.06.2012 TMB: la til attributtet seksjon i kjelde.
      -  rettelse 30.04.2013 TMB: flyttet biletlenke fremst i handlingen.
      -  rettelse 02.05.2013 TMB: tillater biletlenke både fremst og sist i handlingsdelen.
      -  utvidelse 07.10.2013 TMB: legger til elementet RHD_ID i alle hendelser, det er for å
      kunne ta vare på informasjon fra RHD som har lenket personer sammen. Det er BARE id som legges til her, selve lenkingen gjøres i eksterne filer som peker til denne IDen. 
-->
<!-- Overordna element: -->
<grammar xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
    <start combine="choice">
        <ref name="kyrre"></ref>
    </start>
    
  <include href="generelt.rng"/>
  <define name="kyrre">
    <element name="kyrre">
      <ref name="attlist.kyrre"/>
        
      <ref name="meta"/>
      
      <zeroOrMore>
        <ref name="dp"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="kf"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="vi"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="gr"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="df"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="va"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="if"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="uf"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="pa"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="ko"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="in"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="im"/>
      </zeroOrMore>
      <zeroOrMore>
        <ref name="um"/>
      </zeroOrMore>
    </element>
  </define>
  
  
  <define name="attlist.kyrre" combine="interleave">
    <optional>
      <attribute name="versjon" a:defaultValue="1.2">
        <value type="string">1.2</value>
      </attribute>
    </optional>
  </define>

    <!-- Nye meta-elementer -->
    <define name="meta" combine="choice">
        <element name="meta">
            <zeroOrMore>
                <ref name="tittel"/>
            </zeroOrMore>
            <optional><ref name="filnamn"/></optional>
            <!--<optional><ref name="dokumentasjon"/></optional>-->
            <optional><ref name="lenke"/></optional>
            <optional><ref name="konvertering"/></optional>
            <optional><ref name="publiseringsdato"/></optional>
            <ref name="kjelde"/>
            <ref name="registrering"/>
        </element>
    </define>

    <define name="tittel">
        <element name="tittel">
            <ref name="attlist.tittel"/>
            <text/>
        </element>
    </define>
    
    <define name="attlist.tittel">
        <optional> <!-- Man kan angir hvilket språk en tittel er på. -->
            <attribute name="xml:lang">
                <data type="language">
                    <param name="length">2</param>
                </data>
            </attribute>
        </optional>
    </define>
    
    <define name="filnamn">
        <element name="filnamn">
            <text/>
        </element>
    </define> 
    <define name="konvertering">
        <element name="konvertering">
            <text/>
        </element>
    </define>
    
    <define name="publiseringsdato">
        <element name="publiseringsdato">
            <text/>
        </element>
    </define>
    <define name="dokumentasjon">
        <element name="dokumentasjon">
            <ref name="tekst"></ref>
        </element>
        <optional><ref name="lenke"/></optional>
        <optional><ref name="img"/></optional>
    </define>
  <!-- Versjonsnummer av KYRRE -->
  <!-- Felles underelement for fleire hovudelement: -->
    <define name="merknader" combine="choice">
    <element name="merknader">
      <ref name="attlist.merknader"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.merknader" combine="interleave">
        <ref name="attlist.global"/> 
        <empty/>
  </define>
  <!-- Evt. merknader til kjelda, registreringa eller ei handling -->
    <define name="side" combine="choice">
    <element name="side">
      <ref name="attlist.side"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.side" combine="interleave">
      <ref name="attlist.global"/>
      <empty/>
  </define>
  <!-- Sidetalet i kyrkjeboka -->
  <define name="lopenr">
    <element name="lopenr">
      <ref name="attlist.lopenr"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.lopenr" combine="interleave">
        <ref name="attlist.global"/>
    <empty/>
  </define>
  <!-- Handlinga sitt loepenummer i kyrkjeboka, innafor aaret -->
    <define name="aar" combine="choice">
    <element name="aar">
      <ref name="attlist.aar"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.aar" combine="interleave">
        <ref name="attlist.global"/>
    <empty/>
  </define>
  <!-- Aaret handlinga vart foert i kyrkjeboka -->
  <define name="sokn_kyrkje">
    <element name="sokn_kyrkje">
      <ref name="attlist.sokn_kyrkje"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.sokn_kyrkje" combine="interleave">
        <ref name="attlist.global"/>
    <empty/>
  </define>
  <!-- Namn paa sokn eller kyrkje for handlinga -->
  <define name="sokn">
    <element name="sokn">
      <ref name="attlist.sokn"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.sokn" combine="interleave">
        <ref name="attlist.global"/>
    <empty/>
  </define>
  <!-- Namn paa sokn for verdsleg handling -->
  <define name="meldar">
    <element name="meldar">
      <ref name="attlist.meldar"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.meldar" combine="interleave">
        <ref name="attlist.global"/>
    <empty/>
  </define>
  <!-- Opplysningar om melding av doedfoedsel -->
  <define name="rolle">
    <element name="rolle">
      <ref name="attlist.rolle"/>
      <ref name="verdier.roller"/>
    </element>
  </define>
  <define name="attlist.rolle" combine="interleave">
        <ref name="attlist.global"/>
    <empty/>
  </define>
  <!--
    Personen si rolle i handlinga. Lovlige verdiar: barn, far, mor,
    fadder, heimedoeypar, daapsvitne, daapsmeldar, konfirmant, brudgom,
    brur, brudgommens far, bruras far, trulovar, lysingsforlangar,
    avdoed, paaroerande, meldar, foedselshjelpar, vaksinert, vaksinatoer,
    innflyttar, utflyttar, syndar, kommunikant, introdusert, ektemann,
    innmeld, utmeld
  -->
  <define name="stilling_stand">
    <element name="stilling_stand">
      <ref name="attlist.stilling_stand"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.stilling_stand" combine="interleave">
        <ref name="attlist.global"/>
    <empty/>
  </define>
  <!-- Personen si stilling eller stand viss oppgjeve i kjelda -->
  <define name="forenamn">
    <element name="forenamn">
      <ref name="attlist.forenamn"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.forenamn" combine="interleave">
        <ref name="attlist.global"/>
    <empty/>
  </define>
  <!-- Personen sitt foerenamn -->
  <define name="etternamn">
    <element name="etternamn">
      <ref name="attlist.etternamn"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.etternamn" combine="interleave">
        <ref name="attlist.global"/>
    <empty/>
  </define>
  <!-- Personen sitt etternamn -->
  <define name="kjonn">
    <element name="kjonn">
      <ref name="attlist.kjonn"/>
      <ref name="verdier.kjonn"/>
    </element>
  </define>
  <define name="attlist.kjonn" combine="interleave">
        <ref name="attlist.global"/>
    <empty/>
  </define>
  <!-- Personen sitt kjoenn. Lovlige verdiar: m, k, ??, m!!, k!! -->
  <define name="bustad">
    <element name="bustad">
      <ref name="attlist.bustad"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.bustad" combine="interleave">
        <ref name="attlist.global"/>
    <empty/>
  </define>
  <!-- Personen sin bustad eller opphaldsstad -->
  <define name="fodestad">
    <element name="fodestad">
      <ref name="attlist.fodestad"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.fodestad" combine="interleave">
        <ref name="attlist.global"/>
    <empty/>
  </define>
  <!-- Personen sin foedestad -->
  <define name="fodselsdato">
    <element name="fodselsdato">
      <ref name="attlist.fodselsdato"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.fodselsdato" combine="interleave">
        <ref name="attlist.global"/>
    <empty/>
  </define>
  <!-- Personen sin foedselsdato viss oppgjeve, format: dd.mm -->
  <define name="fodselsaar">
    <element name="fodselsaar">
      <ref name="attlist.fodselsaar"/>
      <ref name="tekst"/>
    </element>
  </define>
  <!-- Personen sitt foedselsaar viss oppgjeve, format: 4 siffer -->
  <define name="attlist.fodselsaar" combine="choice">
        <ref name="attlist.global"/>
    <optional>
      <attribute name="grunnlag"/>
    </optional>
  </define>
  <!-- Grunnlag for verdien i 'fodselsaar', f.eks. 'alder' -->
  <define name="alder">
    <element name="alder">
      <ref name="attlist.alder"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.alder" combine="interleave">
        <ref name="attlist.global"/>
    <empty/>
  </define>
  <!-- Personen sin alder viss oppgjeve -->
  <define name="ekte_uekte">
    <element name="ekte_uekte">
      <ref name="attlist.ekte_uekte"/>
      <ref name="verdier.ekte_uekte"/>
        <!--<choice>
            <value>hei</value>
            <value>hå</value>
        </choice>-->
    </element>
  </define>
  <define name="attlist.ekte_uekte" combine="interleave">
        <ref name="attlist.global"/>
    <empty/>
  </define>
  <!-- Om barnet er ekte eller uekte foedd. Verdiar: E eller U -->
  <define name="vaksin">
    <element name="vaksin">
      <ref name="attlist.vaksin"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.vaksin" combine="interleave">
        <ref name="attlist.global"/>
    <empty/>
  </define>
  <!--
    Opplysningar om konfirmanten, brudgommen eller brura sin koppe-
    vaksinasjon etc.
  -->
  <define name="kjeldeside">
    <element name="kjeldeside">
      <ref name="attlist.kjeldeside"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.kjeldeside" combine="interleave">
        <ref name="attlist.global"/>
    <empty/>
  </define>
  <!-- Kjeldesidenummer (indeks) til skanna bilete i Digitalarkivet -->
  <define name="biletlenke">
    <element name="biletlenke">
      <ref name="attlist.biletlenke"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.biletlenke" combine="interleave">
        <ref name="attlist.global"/>
    <empty/>
  </define>
  <!-- URL til skanna bilete i Digitalarkivet -->
  <!-- Dokumentasjon om kyrkjeboka og den aktuelle lista i denne: -->
  <define name="kjelde" combine="choice">
    <element name="kjelde">
      <ref name="attlist.kjelde"/>
      <ref name="prgjeld"/>
      <optional>
        <ref name="ksokn"/>
      </optional>
      <ref name="kommnr"/>
      <optional>
        <ref name="soknenr"/>
      </optional>
      <ref name="ktype"/>
      <optional>
        <ref name="signatur"/>
      </optional>
      <ref name="ltype"/>
      <ref name="startaar"/>
      <ref name="sluttaar"/>
      <optional>
        <ref name="oppbstad"/>
      </optional>
      <optional>
        <ref name="merknader"/>
      </optional>
    </element>
  </define>
  <define name="attlist.kjelde" combine="interleave">
      <attribute name="kjeldeid"/>
      <optional> 
          <attribute name="gda_kid"/>
      </optional>
      <optional>
          <attribute name="seksjon"/>
      </optional>
  </define>
  <!-- Kjeldas ID i Digitalarkivet -->
    <define name="prgjeld" combine="choice">
    <element name="prgjeld">
      <ref name="attlist.prgjeld"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.prgjeld" combine="interleave">
    <optional>
      <ref name="attlist.global"></ref>
    </optional>
  </define>
  <!-- Prestegjeldets eller byens namn -->
  <define name="ksokn">
    <element name="ksokn">
      <ref name="attlist.ksokn"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.ksokn" combine="interleave">
    <empty/>
  </define>
  <!-- Soknets eller kyrkjelydens namn -->
    <define name="kommnr" combine="choice">
    <element name="kommnr">
      <ref name="attlist.global"></ref>
      <ref name="attlist.kommnr"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.kommnr" combine="interleave">
    <empty/>
  </define>
  <!-- Kommunenummer til prestegjeldet eller soknet -->
    <define name="soknenr" combine="choice">
    <element name="soknenr">
      <ref name="attlist.soknenr"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.soknenr" combine="interleave">
    <empty/>
  </define>
  <!-- Soknenummer til prestegjeldet (6 siffer) eller soknet (8 s.) -->
    <define name="ktype" combine="choice">
    <element name="ktype">
      <ref name="attlist.ktype"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.ktype" combine="interleave">
    <empty/>
  </define>
  <!-- Kjeldetype ): MINI, KLOK, FREG, LYSN osb. -->
    <define name="signatur" combine="choice">
    <element name="signatur">
      <ref name="attlist.signatur"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.signatur" combine="interleave">
    <empty/>
  </define>
  <!-- Arkivsignatur ): referanse/nummer i Arkivverkets katalog -->
  <define name="ltype">
    <element name="ltype">
      <ref name="attlist.ltype"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.ltype" combine="interleave">
    <empty/>
  </define>
  <!-- Listetype ): dp, kf, vi, gr, df, va, if, uf osb. -->
  <define name="startaar">
    <element name="startaar">
      <ref name="attlist.startaar"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.startaar" combine="interleave">
    <empty/>
  </define>
  <!-- Startaar for (kjelda eller) lista -->
  <define name="sluttaar">
    <element name="sluttaar">
      <ref name="attlist.sluttaar"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.sluttaar" combine="interleave">
    <empty/>
  </define>
  <!-- Sluttaar for (kjelda eller) lista -->
  <define name="oppbstad">
    <element name="oppbstad">
      <ref name="attlist.oppbstad"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.oppbstad" combine="interleave">
    <empty/>
  </define>
  <!-- Oppbevaringsstad for originalkjelda ): statsarkivkode e.a. -->
  <!-- Dokumentasjon om registreringa: -->
  <define name="registrering">
    <element name="registrering">
      <ref name="attlist.registrering"/>
      <optional>
        <ref name="reg_av"/>
      </optional>
      <optional>
        <ref name="reg_naar"/>
      </optional>
      <optional>
        <ref name="korr_av"/>
      </optional>
      <optional>
        <ref name="korr_naar"/>
      </optional>
      <ref name="foretak"/>
      <optional>
        <ref name="kontakt"/>
      </optional>
      <optional>
        <ref name="grunnlag"/>
      </optional>
      <optional>
        <ref name="program"/>
      </optional>
      <optional>
        <ref name="fildato"/>
      </optional>
      <optional>
        <ref name="merknader"/>
      </optional>
    </element>
  </define>
  <define name="attlist.registrering" combine="interleave">
    <empty/>
  </define>
  <define name="reg_av">
    <element name="reg_av">
      <ref name="attlist.reg_av"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.reg_av" combine="interleave">
    <empty/>
  </define>
  <!-- Namn paa person(er) som har registrert kjelda/lista -->
  <define name="reg_naar">
    <element name="reg_naar">
      <ref name="attlist.reg_naar"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.reg_naar" combine="interleave">
    <empty/>
  </define>
  <!-- Aar for eller anna tidfesting av registreringsarbeidet -->
  <define name="korr_av">
    <element name="korr_av">
      <ref name="attlist.korr_av"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.korr_av" combine="interleave">
    <empty/>
  </define>
  <!-- Namn paa person(er) som har korrekturlese kjelda/lista -->
  <define name="korr_naar">
    <element name="korr_naar">
      <ref name="attlist.korr_naar"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.korr_naar" combine="interleave">
    <empty/>
  </define>
  <!-- Aar for eller anna tidfesting av korrekturarbeidet -->
  <define name="foretak">
    <element name="foretak">
      <ref name="attlist.foretak"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.foretak" combine="interleave">
    <empty/>
  </define>
  <!--
    Organisasjon/institusjon eller person som staar bak
    registreringa; rettshavar
  -->
  <define name="kontakt">
    <element name="kontakt">
      <ref name="attlist.kontakt"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.kontakt" combine="interleave">
    <empty/>
  </define>
  <!-- Namn (evt.), adresse, telefon eller e-post til kontaktperson -->
  <define name="grunnlag">
    <element name="grunnlag">
      <ref name="attlist.grunnlag"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.grunnlag" combine="interleave">
    <empty/>
  </define>
  <!--
    Grunnlag for registreringa ): Papirkopi, Mikrofilm, Mikrofiche,
    Skanning eller Original
  -->
  <define name="program">
    <element name="program">
      <ref name="attlist.program"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.program" combine="interleave">
    <empty/>
  </define>
  <!-- Registreringsprogram ): Augustus, Access, Excel, BD87 osb. -->
  <define name="fildato">
    <element name="fildato">
      <ref name="attlist.fildato"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.fildato" combine="interleave">
    <empty/>
  </define>
  <!-- Dato for ferdigstilling av datafila, format: "dd.mm aaaa" -->
  <!-- Daap: Handling -->
  <define name="dp">
    <element name="dp">
      <ref name="attlist.dp"/>
      <choice>
        <group>
           <optional>
              <ref name="biletlenke"/>
           </optional> 
          <ref name="side"/>
          <optional>
            <ref name="lopenr"/>
          </optional>
          <ref name="aar"/>
          <optional>
            <ref name="dpdato"/>
          </optional>
          <optional>
            <ref name="stadfesta"/>
          </optional>
          <optional>
            <ref name="dpstad"/>
          </optional>
          <optional>
            <ref name="introd"/>
          </optional>
          <optional>
            <ref name="melding"/>
          </optional>
          <optional>
            <ref name="sokn_kyrkje"/>
          </optional>
          <optional>
            <ref name="merknader"/>
          </optional>
          <optional>
            <ref name="kjeldeside"/>
          </optional>
      <optional>
        <ref name="RHD_ID"/>
      </optional>
        </group>
        <group>
          <ref name="side"/>
          <optional>
            <ref name="lopenr"/>
          </optional>
          <ref name="aar"/>
          <optional>
            <ref name="dpdato"/>
          </optional>
          <optional>
            <ref name="stadfesta"/>
          </optional>
          <optional>
            <ref name="dpstad"/>
          </optional>
          <optional>
            <ref name="introd"/>
          </optional>
          <optional>
            <ref name="melding"/>
          </optional>
          <optional>
            <ref name="sokn_kyrkje"/>
          </optional>
          <optional>
            <ref name="merknader"/>
          </optional>
          <optional>
            <ref name="kjeldeside"/>
          </optional>
      <optional>
        <ref name="RHD_ID"/>
      </optional>
          <optional>
            <ref name="biletlenke"/>
          </optional>
        </group>
      </choice>
      <oneOrMore>
        <ref name="dp_person"/>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.dp" combine="interleave">
        <ref name="attlist.global"/>
    <optional>
      <attribute name="dpnr"/>
    </optional>
      <ref name="attlist.id"/>
  </define>
  <!-- Handlingsnummer (daapsnummer) innafor fila -->
  <!-- Globalt unik postID for daapsposten -->
  <define name="dpdato">
    <element name="dpdato">
      <ref name="attlist.dpdato"/>
      <ref name="tekst"/>
    </element>
  </define>
  <define name="attlist.dpdato" combine="interleave">
        <ref name="attlist.global"/>
    <empty/>
