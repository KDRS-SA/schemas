<?xml version="1.0" encoding="UTF-8"?>
<!--
  DTD for HistformML - en XML-versjon av utvekslingsformatet i Histform - Norsk standard for registrering og utveksling av
  nominative folketellingsdata for aarene 1865-1910. (Registreringssentral for historiske data, Tromsoe, januar 1995,
  ISBN 82-7663-419-2)
  HistformML er noe utvidet i forhold til Histform-standarden som en foelge av harmonisering med kirkebokstandarden Kyrre
  og dennes hovedformat KyrreML. (Digitalarkivet, Bergen, desember 2005)
  Versjon 1.0, Riksarkivet, 27.05.2010 (utarbeidet av Lars Nygaard)
  
  Elementene 'kildeside' og 'bildelenke' lagt til. Attributtverdien 'grunnlag' lagt til i elementet 'fodselsaar'.
  Versjon 1.1, Riksarkivet, 17.06.2011 (revidert av Lars Nygaard)
-->
<!-- Overordnet element: -->
<grammar xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="">
  <define name="histform">
    <element name="histform">
      <ref name="attlist.histform"/>
      <ref name="kilde"/>
      <ref name="registrering"/>
      <oneOrMore>
        <choice>
          <ref name="tellingskrets"/>
          <ref name="informasjon"/>
        </choice>
      </oneOrMore>
    </element>
  </define>
  <define name="attlist.histform" combine="interleave">
    <optional>
      <attribute name="versjon" a:defaultValue="1.1">
        <value type="string">1.1</value>
      </attribute>
    </optional>
  </define>
  <!-- Versjonsnummer av HistformML -->
  <!-- Felles feltelementer for flere postelementer: -->
  <define name="kretsnummer">
    <element name="kretsnummer">
      <ref name="attlist.kretsnummer"/>
      <text/>
    </element>
  </define>
  <define name="attlist.kretsnummer" combine="interleave">
    <empty/>
  </define>
  <!-- Tellingskretsens nummer i kilden -->
  <define name="bostedsnummer">
    <element name="bostedsnummer">
      <ref name="attlist.bostedsnummer"/>
      <text/>
    </element>
  </define>
  <define name="attlist.bostedsnummer" combine="interleave">
    <empty/>
  </define>
  <!-- Bostedets (gaardens/husets) listenummer i kilden -->
  <define name="leilighetsnummer">
    <element name="leilighetsnummer">
      <ref name="attlist.leilighetsnummer"/>
      <text/>
    </element>
  </define>
  <define name="attlist.leilighetsnummer" combine="interleave">
    <empty/>
  </define>
  <!-- Leilighetens listenummer i kilden -->
  <define name="personnummer">
    <element name="personnummer">
      <ref name="attlist.personnummer"/>
      <text/>
    </element>
  </define>
  <define name="attlist.personnummer" combine="interleave">
    <empty/>
  </define>
  <!-- Personens nummer innenfor bostedet/leiligheten/skipet -->
  <define name="antall_tilstedevarende_personer">
    <element name="antall_tilstedevarende_personer">
      <ref name="attlist.antall_tilstedevarende_personer"/>
      <text/>
    </element>
  </define>
  <define name="attlist.antall_tilstedevarende_personer" combine="interleave">
    <empty/>
  </define>
  <!-- Antall tilstedevaerende personer -->
  <define name="antall_hjemmehorende_personer">
    <element name="antall_hjemmehorende_personer">
      <ref name="attlist.antall_hjemmehorende_personer"/>
      <text/>
    </element>
  </define>
  <define name="attlist.antall_hjemmehorende_personer" combine="interleave">
    <empty/>
  </define>
  <!-- Antall hjemmehoerende personer -->
  <define name="merknader">
    <element name="merknader">
      <ref name="attlist.merknader"/>
      <text/>
    </element>
  </define>
  <define name="attlist.merknader" combine="interleave">
    <empty/>
  </define>
  <!-- Evt. merknader til kilden, registreringen, bosteder eller personer -->
  <define name="side">
    <element name="side">
      <ref name="attlist.side"/>
      <text/>
    </element>
  </define>
  <define name="attlist.side" combine="interleave">
    <empty/>
  </define>
  <!-- Sidetallet i folketellingslisten -->
  <define name="kildeside">
    <element name="kildeside">
      <ref name="attlist.kildeside"/>
      <text/>
    </element>
  </define>
  <define name="attlist.kildeside" combine="interleave">
    <empty/>
  </define>
  <!-- Kildesidenummer (indeks) til skannet bilde i Digitalarkivet -->
  <define name="bildelenke">
    <element name="bildelenke">
      <ref name="attlist.bildelenke"/>
      <text/>
    </element>
  </define>
  <define name="attlist.bildelenke" combine="interleave">
    <empty/>
  </define>
  <!-- URL til skannet bilde i Digitalarkivet -->
  <!-- Dokumentasjon om den aktuelle folketellingen paa kommune- eller prestegjeldsnivaa: -->
  <define name="kilde">
    <element name="kilde">
      <ref name="attlist.kilde"/>
      <choice>
        <ref name="kommune"/>
        <ref name="prgjeld"/>
      </choice>
      <ref name="kommnr"/>
      <optional>
        <ref name="soknenr"/>
      </optional>
      <ref name="ktype"/>
      <ref name="aar"/>
      <optional>
        <ref name="dato"/>
      </optional>
      <optional>
        <ref name="signatur"/>
      </optional>
      <optional>
        <ref name="asta_streng"/>
      </optional>
      <optional>
        <ref name="oppbsted"/>
      </optional>
      <optional>
        <ref name="merknader"/>
      </optional>
    </element>
  </define>
  <define name="attlist.kilde" combine="interleave">
    <attribute name="kildeid"/>
  </define>
  <!-- Kildens ID i Digitalarkivet -->
  <define name="kommune">
    <element name="kommune">
      <ref name="attlist.kommune"/>
      <text/>
    </element>
  </define>
  <define name="attlist.kommune" combine="interleave">
    <empty/>
  </define>
  <!-- Herredets eller byens navn -->
  <define name="prgjeld">
    <element name="prgjeld">
      <ref name="attlist.prgjeld"/>
      <text/>
    </element>
  </define>
  <define name="attlist.prgjeld" combine="interleave">
    <empty/>
  </define>
  <!-- Prestegjeldets eller byens navn -->
  <define name="kommnr">
    <element name="kommnr">
      <ref name="attlist.kommnr"/>
      <text/>
    </element>
  </define>
  <define name="attlist.kommnr" combine="interleave">
    <empty/>
  </define>
  <!-- Kommunenummer til kommunen eller prestegjeldet -->
  <define name="soknenr">
    <element name="soknenr">
      <ref name="attlist.soknenr"/>
      <text/>
    </element>
  </define>
  <define name="attlist.soknenr" combine="interleave">
    <empty/>
  </define>
  <!-- Soknenummer til prestegjeldet (6 siffer) -->
  <define name="ktype">
    <element name="ktype">
      <ref name="attlist.ktype"/>
      <text/>
    </element>
  </define>
  <define name="attlist.ktype" combine="interleave">
    <empty/>
  </define>
  <!-- Kildetype ): FOLK -->
  <define name="aar">
    <element name="aar">
      <ref name="attlist.aar"/>
      <text/>
    </element>
  </define>
  <define name="attlist.aar" combine="interleave">
    <empty/>
  </define>
  <!-- Folketellingsaaret med 4 siffer -->
  <define name="dato">
    <element name="dato">
      <ref name="attlist.dato"/>
      <text/>
    </element>
  </define>
  <define name="attlist.dato" combine="interleave">
    <empty/>
  </define>
  <!-- Folketellingsdatoen paa formatet "mm.dd" -->
  <define name="signatur">
    <element name="signatur">
      <ref name="attlist.signatur"/>
      <text/>
    </element>
  </define>
  <define name="attlist.signatur" combine="interleave">
    <empty/>
  </define>
  <!-- Arkivsignatur ): gammelt katalognummer i Arkivverket -->
  <define name="asta_streng">
    <element name="asta_streng">
      <ref name="attlist.asta_streng"/>
      <text/>
    </element>
  </define>
  <define name="attlist.asta_streng" combine="interleave">
    <empty/>
  </define>
  <!-- Asta-streng ): nytt katalognummer paa formen S-1344/Hab/L0001 -->
  <define name="oppbsted">
    <element name="oppbsted">
      <ref name="attlist.oppbsted"/>
      <text/>
    </element>
  </define>
  <define name="attlist.oppbsted" combine="interleave">
    <empty/>
  </define>
  <!-- Oppbevaringssted for originalkilden ): statsarkivkode -->
  <!-- Dokumentasjon om registreringen: -->
  <define name="registrering">
    <element name="registrering">
      <ref name="attlist.registrering"/>
      <optional>
        <ref name="reg_av"/>
      </optional>
      <optional>
        <ref name="reg_naar"/>
      </optional>
      <optional>
        <ref name="korr_av"/>
      </optional>
      <optional>
        <ref name="korr_naar"/>
      </optional>
      <ref name="foretak"/>
      <optional>
        <ref name="kontakt"/>
      </optional>
      <optional>
        <ref name="grunnlag"/>
      </optional>
      <optional>
        <ref name="program"/>
      </optional>
      <optional>
        <ref name="fildato"/>
      </optional>
      <optional>
        <ref name="merknader"/>
      </optional>
    </element>
  </define>
  <define name="attlist.registrering" combine="interleave">
    <empty/>
  </define>
  <define name="reg_av">
    <element name="reg_av">
      <ref name="attlist.reg_av"/>
      <text/>
    </element>
  </define>
  <define name="attlist.reg_av" combine="interleave">
    <empty/>
  </define>
  <!-- Navn paa person(er) eller foretak som har registrert kilden -->
  <define name="reg_naar">
    <element name="reg_naar">
      <ref name="attlist.reg_naar"/>
      <text/>
    </element>
  </define>
  <define name="attlist.reg_naar" combine="interleave">
    <empty/>
  </define>
  <!-- Aar for eller annen tidfesting av registreringsarbeidet -->
  <define name="korr_av">
    <element name="korr_av">
      <ref name="attlist.korr_av"/>
      <text/>
    </element>
  </define>
  <define name="attlist.korr_av" combine="interleave">
    <empty/>
  </define>
  <!-- Navn paa person(er) eller foretak som har korrekturlest kilden -->
  <define name="korr_naar">
    <element name="korr_naar">
      <ref name="attlist.korr_naar"/>
      <text/>
    </element>
  </define>
  <define name="attlist.korr_naar" combine="interleave">
    <empty/>
  </define>
  <!-- Aar for eller annen tidfesting av korrekturarbeidet -->
  <define name="foretak">
    <element name="foretak">
      <ref name="attlist.foretak"/>
      <text/>
    </element>
  </define>
  <define name="attlist.foretak" combine="interleave">
    <empty/>
  </define>
  <!--
    Organisasjon/institusjon eller person som staar bak
    registreringen; rettighetshaver
  -->
  <define name="kontakt">
    <element name="kontakt">
      <ref name="attlist.kontakt"/>
      <text/>
    </element>
  </define>
  <define name="attlist.kontakt" combine="interleave">
    <empty/>
  </define>
  <!-- Navn (evt.), adresse, telefon eller e-post til kontaktperson -->
  <define name="grunnlag">
    <element name="grunnlag">
      <ref name="attlist.grunnlag"/>
      <text/>
    </element>
  </define>
  <define name="attlist.grunnlag" combine="interleave">
    <empty/>
  </define>
  <!--
    Kildegrunnlag for registreringen ): Papirkopi, Mikrofilm, Mikrofiche,
    Skanning eller Original
  -->
  <define name="program">
    <element name="program">
      <ref name="attlist.program"/>
      <text/>
    </element>
  </define>
  <define name="attlist.program" combine="interleave">
    <empty/>
  </define>
  <!-- Registreringsprogram ): CensIn, Access, Excel, BD87 osv. -->
  <define name="fildato">
    <element name="fildato">
      <ref name="attlist.fildato"/>
      <text/>
    </element>
  </define>
  <define name="attlist.fildato" combine="interleave">
    <empty/>
  </define>
  <!-- Dato for ferdigstilling av datafilen, format: "yyyy.mm.dd" -->
  <!-- Tellingskrets (1) -->
  <define name="tellingskrets">
    <element name="tellingskrets">
      <ref name="attlist.tellingskrets"/>
      <ref name="kretsnummer"/>
      <optional>
        <ref name="tellingskretsens_navn"/>
      </optional>
      <optional>
        <ref name="sogn"/>
      </optional>
      <optional>
        <ref name="prestegjeld"/>
      </optional>
      <optional>
        <ref name="herred_by"/>
      </optional>
      <optional>
        <ref name="merknader"/>
      </optional>
      <optional>
        <ref name="kildeside"/>
      </optional>
      <optional>
        <ref name="bildelenke"/>
      </optional>
      <zeroOrMore>
        <choice>
          <ref name="bosted_land"/>
          <ref name="bosted_by"/>
          <ref name="skip"/>
          <ref name="informasjon"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.tellingskrets" combine="interleave">
    <optional>
      <attribute name="id"/>
    </optional>
  </define>
  <!-- Globalt unik postID for kretsposten -->
  <define name="tellingskretsens_navn">
    <element name="tellingskretsens_navn">
      <ref name="attlist.tellingskretsens_navn"/>
      <text/>
    </element>
  </define>
  <define name="attlist.tellingskretsens_navn" combine="interleave">
    <empty/>
  </define>
  <!-- Tellingskretsens navn i kilden -->
  <define name="sogn">
    <element name="sogn">
      <ref name="attlist.sogn"/>
      <text/>
    </element>
  </define>
  <define name="attlist.sogn" combine="interleave">
    <empty/>
  </define>
  <!-- Denne kretsens tilhoerighet til sokn -->
  <define name="prestegjeld">
    <element name="prestegjeld">
      <ref name="attlist.prestegjeld"/>
      <text/>
    </element>
  </define>
  <define name="attlist.prestegjeld" combine="interleave">
    <empty/>
  </define>
  <!-- Denne kretsens tilhoerighet til prestegjeld -->
  <define name="herred_by">
    <element name="herred_by">
      <ref name="attlist.herred_by"/>
      <text/>
    </element>
  </define>
  <define name="attlist.herred_by" combine="interleave">
    <empty/>
  </define>
  <!-- Denne kretsens tilhoerighet til herred eller by -->
  <!-- Bosted_land (2a) -->
  <define name="bosted_land">
    <element name="bosted_land">
      <ref name="attlist.bosted_land"/>
      <ref name="bostedsnummer"/>
      <optional>
        <ref name="bostedets_navn"/>
      </optional>
      <optional>
        <ref name="matrnr_gnr"/>
      </optional>
      <optional>
        <ref name="lopenr_bnr"/>
      </optional>
      <optional>
        <ref name="antall_tilstedevarende_personer"/>
      </optional>
      <optional>
        <ref name="antall_hjemmehorende_personer"/>
      </optional>
      <optional>
        <ref name="antall_personsedler"/>
      </optional>
      <optional>
        <ref name="antall_husholdninger"/>
      </optional>
      <optional>
        <ref name="sidebygning_med_natteopphold"/>
      </optional>
      <optional>
        <ref name="antall_slike_bygninger"/>
      </optional>
      <optional>
        <ref name="hvilke_slike_bygninger"/>
      </optional>
      <optional>
        <ref name="jordbrukssporsmaal"/>
      </optional>
      <optional>
        <ref name="antall_personer_i_fjos"/>
      </optional>
      <optional>
        <ref name="antall_personer_i_stall"/>
      </optional>
      <optional>
        <ref name="tredje_sidebygning"/>
      </optional>
      <optional>
        <ref name="antall_personer_i_tredje_sidebygning"/>
      </optional>
      <optional>
        <ref name="fjerde_sidebygning"/>
      </optional>
      <optional>
        <ref name="antall_personer_i_fjerde_sidebygning"/>
      </optional>
      <optional>
        <ref name="merknader"/>
      </optional>
      <optional>
        <ref name="kildeside"/>
      </optional>
      <optional>
        <ref name="bildelenke"/>
      </optional>
      <zeroOrMore>
        <choice>
          <ref name="person"/>
          <ref name="hjemvendt_norsk_amerikaner"/>
          <ref name="jordbruk"/>
          <ref name="informasjon"/>
        </choice>
      </zeroOrMore>
    </element>
  </define>
  <define name="attlist.bosted_land" combine="interleave">
    <optional>
      <attribute name="id"/>
    </optional>
  </define>
  <!-- Globalt unik postID for bostedposten -->
  <define name="bostedets_navn">
    <element name="bostedets_navn">
      <ref name="attlist.bostedets_navn"/>
      <text/>
    </element>
  </define>
  <define name="attlist.bostedets_navn" combine="interleave">
    <empty/>
  </define>
  <!-- Bostedets navn i kilden -->
  <define name="matrnr_gnr">
    <element name="matrnr_gnr">
      <ref name="attlist.matrnr_gnr"/>
      <text/>
    </element>
  </define>
  <define name="attlist.matrnr_gnr" combine="interleave">
    <empty/>
  </define>
  <!-- Bostedets matrikkelnummer eller gaardsnummer -->
  <define name="lopenr_bnr">
    <element name="lopenr_bnr">
      <ref name="attlist.lopenr_bnr"/>
      <text/>
    </element>
  </define>
  <define name="attlist.lopenr_bnr" combine="interleave">
    <empty/>
  </define>
  <!-- Bostedets loepenummer eller bruksnummer -->
  <define name="antall_personsedler">
    <element name="antall_personsedler">
      <ref name="attlist.antall_personsedler"/>
      <text/>
    </element>
  </define>
  <define name="attlist.antall_personsedler" combine="interleave">
    <empty/>
  </define>
  <!-- Antall personsedler (1891) -->
  <define name="antall_husholdninger">
    <element name="antall_husholdninger">
      <ref name="attlist.antall_husholdninger"/>
      <text/>
    </element>
  </define>
  <define name="attlist.antall_husholdninger" combine="interleave">
    <empty/>
  </define>
  <!-- Antall husholdninger -->
  <define name="sidebygning_med_natteopphold">
    <element name="sidebygning_med_natteopphold">
      <ref name="attlist.sidebygning_med_natteopphold"/>
      <text/>
    </element>
  </define>
  <define name="attlist.sidebygning_med_natteopphold" combine="interleave">
    <empty/>
  </define>
  <!-- Side-/uthusbygning med natteopphold? (j/n) -->
  <define name="antall_slike_bygninger">
    <element name="antall_slike_bygninger">
      <ref name="attlist.antall_slike_bygninger"/>
      <text/>
    </element>
  </define>
  <define name="attlist.antall_slike_bygninger" combine="interleave">
    <empty/>
  </define>
  <!-- Antall slike bygninger -->
  <define name="hvilke_slike_bygninger">
    <element name="hvilke_slike_bygninger">
      <ref name="attlist.hvilke_slike_bygninger"/>
      <text/>
    </element>
  </define>
  <define name="attlist.hvilke_slike_bygninger" combine="interleave">
    <empty/>
  </define>
  <!-- Hvilke slike bygninger -->
  <define name="jordbrukssporsmaal">
    <element name="jordbrukssporsmaal">
      <ref name="attlist.jordbrukssporsmaal"/>
      <text/>
    </element>
  </define>
  <define name="attlist.jordbrukssporsmaal" combine="interleave">
    <empty/>
  </define>
  <!-- Jordbruksspoersmaal -->
  <define name="antall_personer_i_fjos">
    <element name="antall_personer_i_fjos">
      <ref name="attlist.antall_personer_i_fjos"/>
      <text/>
    </element>
  </define>
  <define name="attlist.antall_personer_i_fjos" combine="interleave">
    <empty/>
  </define>
  <!-- Antall personer i fjoes -->
  <define name="antall_personer_i_stall">
    <element name="antall_personer_i_stall">
      <ref name="attlist.antall_personer_i_stall"/>
      <text/>
    </element>
  </define>
  <define name="attlist.antall_personer_i_stall" combine="interleave">
    <empty/>
  </define>
  <!-- Antall personer i stall -->
  <define name="tredje_sidebygning">
    <element name="tredje_sidebygning">
      <ref name="attlist.tredje_sidebygning"/>
      <text/>
    </element>
  </define>
  <define name="attlist.tredje_sidebygning" combine="interleave">
    <empty/>
  </define>
  <!-- 3. sidebygning -->
  <define name="antall_personer_i_tredje_sidebygning">
    <element name="antall_personer_i_tredje_sidebygning">
      <ref name="attlist.antall_personer_i_tredje_sidebygning"/>
      <text/>
    </element>
  </define>
  <define name="attlist.antall_personer_i_tredje_sidebygning" combine="interleave">
    <empty/>
  </define>
  <!-- Antall personer i 3. sidebygning -->
  <define name="fjerde_sidebygning">
    <element name="fjerde_sidebygning">
      <ref name="attlist.fjerde_sidebygning"/>
      <text/>
    </element>
  </define>
  <define name="attlist.fjerde_sidebygning" combine="interleave">
    <empty/>
  </define>
  <!-- 4. sidebygning -->
  <define name="antall_personer_i_fjerde_sidebygning">
    <element name="antall_personer_i_fjerde_sidebygning">
      <ref name="attlist.antall_personer_i_fjerde_sidebygning"/>
      <text/>
