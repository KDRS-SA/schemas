  </define>
  <define name="attlist.antall_personer_i_fjerde_sidebygning" combine="interleave">
    <empty/>
  </define>
  <!-- Antall personer i 4. sidebygning -->
  <!-- Bosted_by (2b) -->
  <define name="bosted_by">
    <element name="bosted_by">
      <ref name="attlist.bosted_by"/>
      <ref name="bostedsnummer"/>
      <optional>
        <ref name="gatenavn_og_nummer_i_gata"/>
      </optional>
      <optional>
        <ref name="bydel"/>
      </optional>
      <optional>
        <ref name="gaardens_nummer"/>
      </optional>
      <optional>
        <ref name="gaardens_eier"/>
      </optional>
      <optional>
        <ref name="antall_tilstedevarende_personer"/>
      </optional>
      <optional>
        <ref name="antall_hjemmehorende_personer"/>
      </optional>
